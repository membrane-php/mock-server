{
  "name": "membrane/mock-server",
  "type": "library",
  "license": "MPL-2.0",
  "autoload": {
    "psr-4": {
      "Membrane\\MockServer\\": "src/",
      "Membrane\\MockServer\\Generated\\": "generated/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Membrane\\MockServer\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": "^8.3.0",
    "atto/framework": "^0.1",
    "atto/membrane-module": "^0.1.7",
    "membrane/membrane": "^0.8",
    "membrane/openapi-reader": "^2.0",
    "membrane/openapi-router": "0.5.1",
    "symfony/console": "^6.2 || ^7.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^11.5.39",
    "phpstan/phpstan": "^1.12.5",
    "squizlabs/php_codesniffer": "^3.7",
    "guzzlehttp/psr7": "^2.4",
    "mikey179/vfsstream": "^1.6.7"
  },
  "bin": [
    "bin/membrane-mock-server"
  ],
  "config": {
    "allow-plugins": {
      "infection/extension-installer": true
    }
  }
}
